.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
}

.item {
  margin-bottom: 20rpx;
  display: flex;
  align-items: center; /* Center items vertically */
  text-align: center; /* Center text */
}

.avatar {
  width: 50px;
  height: 50px;
  border-radius: 25px;
  margin-right: 10px; /* Add margin to separate avatar from text */
}

.complete-btn {
  width: 100%;
  height: 90rpx;
  padding: 10rpx;
  background-color: #07c160;
  color: #ffffff;
  border: none;
  border-radius: 4rpx;
  cursor: pointer;
  margin-top: 20rpx;
  box-sizing: border-box;
}
